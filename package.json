{
  "name": "serverless-plugin-api-gateway-integrations",
  "version": "0.0.8",
  "description": "Serverless plugin to add API Gateway integrations",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint 'src/**/*.ts'",
    "build": "tsc -p tsconfig.json",
    "test": "JEST_JUNIT_OUTPUT=./reports/junit/${npm_package_name}-test-results.xml jest",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --runInBand --ci",
    "coverage": "yarn test --coverage",
    "prettier": "prettier --write src/**/*.ts",
    "prettier:ci": "prettier --list-different src/**/*.ts",
    "tag": "git tag \"v$npm_package_version\" && git push --tags",
    "prepublishOnly": "yarn run build"
  },
  "keywords": [
    "serverless",
    "serverless framework",
    "serverless applications",
    "serverless modules",
    "api gateway",
    "integrations",
    "aws",
    "aws lambda",
    "amazon",
    "amazon web services",
    "sqs"
  ],
  "author": "Erez Rokah",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "@types/node": "^16.0.0",
    "@types/serverless": "^1.72.6",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.0.0",
    "jest-junit": "^12.0.0",
    "prettier": "^2.0.0",
    "ts-jest": "^27.0.0",
    "typescript": "^4.0.0"
  },
  "engines": {
    "node": ">=v8.17.0"
  },
  "dependencies": {
    "change-case": "^4.0.0"
  }
}
